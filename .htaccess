<Files .htaccess>
 order allow,deny
 deny from all
</Files>

DirectoryIndex application/start.php?bframework=index
Options -Indexes
Options +FollowSymLinks

<IfModule mod_rewrite.c>
RewriteEngine on

RewriteRule ^page\/([A-Za-z0-9\_\-]+)$ application/start.php?bframework=$1&%{QUERY_STRING}

RewriteRule ^action\/([A-Za-z0-9\_\-]+)$ application/start.php?action=$1&%{QUERY_STRING}

RewriteRule    ^js.js$ application/js/js.php
</IfModule>
